{
  "version": 2,
  "builds": [
    {
      "src": "./server.js",
      "use": "@vercel/node"
    },
    {
      "src": "./public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*\\.(css|js|html|png|jpg))",
      "dest": "/public/$1"
    },
          {
  "src": "/(.*\\.(css|js|html|png|jpg|ico|svg|woff|woff2))",
  "dest": "/public/$1"
}
    
  ]
}
